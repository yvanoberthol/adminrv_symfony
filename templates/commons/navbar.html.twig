<div class="row">
    <div class="col-md-12">
        <div style="margin-top:25px;border-radius:25px;" class=" bg-dark">
            <div class="row mb-4 mt-2">
                <nav class="col-md-12 navbar navbar-icon-top navbar-expand-lg navbar-dark bg-secondary p-3">
                    <a class="navbar-brand" href="{{ path('menu_principal') }}"><span
                                class="fa fa-ambulance badge-info text-danger p-3"></span> {{ 'navbar.title'|trans }}</a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse"
                            data-target="#navbarSupportedContent"
                            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <ul class="navbar-nav mr-auto">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                   data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="fa fa-user-o">
                                    </i>
                                    {{ 'navbar.medecin'|trans }}
                                </a>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                    <a class="dropdown-item" href="{{ path('medecins') }}">{{ 'navbar.medecin.list'|trans }}</a>
                                    <a class="dropdown-item" href="{{ path('form_medecin_add') }}">{{ 'navbar.medecin.add'|trans }}</a>
                                </div>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                   data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="fa fa-file-zip-o">
                                    </i>
                                    {{ 'navbar.specialite'|trans }}
                                </a>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                    <a class="dropdown-item" href="{{ path('specialites') }}">{{ 'navbar.specialite.list'|trans }}</a>
                                    <a class="dropdown-item" href="{{ path('form_specialite_add') }}">{{ 'navbar.specialite.add'|trans }}</a>
                                    <a class="dropdown-item" href="{{ path('ms_stat') }}">{{ 'navbar.specialite.stat'|trans }}</a>
                                </div>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                   data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="fa fa-hourglass-end">
                                    </i>
                                    {{ 'navbar.creneau'|trans }}
                                </a>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                    <a class="dropdown-item" href="{{ path('creneaux') }}">{{ 'navbar.creneau.list'|trans }}</a>
                                    <a class="dropdown-item" href="{{ path('form_creneau_add') }}">{{ 'navbar.creneau.add'|trans }}</a>
                                </div>
                            </li>

                            <li class="nav-item dropdown">
                                <a class="nav-link" href="/patients" role="button" aria-haspopup="true"
                                   aria-expanded="false">
                                    <i class="fa fa-users">
                                    </i>
                                    {{ 'navbar.patient'|trans }}
                                </a>
                            </li>

                            {% if is_granted('ROLE_SUPER_ADMIN') %}
                                <li class="nav-item dropdown">
                                    <a class="nav-link dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                       data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        <i class="fa fa-user">
                                        </i>
                                        {{ 'navbar.user'|trans }}
                                    </a>
                                    <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                        <a class="dropdown-item" href="{{ path('users') }}">{{ 'navbar.user.list'|trans }}</a>
                                        <a class="dropdown-item" href="{{ path('form_user_add') }}">{{ 'navbar.user.add'|trans }}</a>
                                    </div>
                                </li>
                            {% endif %}
                        </ul>
                        <ul class="nav navbar-nav navbar-right">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                   data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <i class="fa fa-user-circle">
                                    </i>
                                    <span>{{ app.token.user.username }}</span>
                                </a>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                    <a class="dropdown-item" href="{{ path('user_form_modif') }}">
                                        <i class="fa fa-calculator">
                                        </i>
                                        {{ 'navbar.compte'|trans }}
                                    </a>
                                    <a class="dropdown-item" href="{{ path('logout') }}">
                                        <i class="fa fa-sign-out">
                                        </i>
                                        {{ 'navbar.compte.logout'|trans }}
                                    </a>
                                </div>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
    </div>
</div>